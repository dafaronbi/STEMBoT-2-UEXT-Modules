#include "wifisynth.h"

int main(void) {

	//initialize GPIO
	gpio_init();

	//initialize SPI
	ssp_init();

	//initialize LCD
	TFT_LCD_INIT();

    // Read clock settings and update SystemCoreClock variable
    SystemCoreClockUpdate();

    fillScreen(0xFEEF);

    while(1);
    return 0;
}
